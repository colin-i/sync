#!/usr/bin/bash

echo "LIBRARY actionswf.dll" > $1
echo "EXPORTS" >> $1
b=`x86_64-w64-mingw32-objdump -p actionswf.dll | grep "\[Ordinal\/Name Pointer\] Table" -n`
b=${b%%:*}
b=`echo $b+1 | bc`
c=`x86_64-w64-mingw32-objdump -p actionswf.dll | tail -n +$b`
for (( ; ; ))
do
	c=${c#*]}
	d=${c%%[*}
	d=`echo $d | sed 's/^ //'`
	e=${d%% *}
	echo $e >> $1
	if [[ $d =~ " " ]]
	then
		break
	fi
done
