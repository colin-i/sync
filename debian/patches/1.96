--- actionswf-1.orig/mrt
+++ actionswf-1/mrt
@@ -1,3 +1,3 @@
 
-cd tests && ./a && ./c 1 && cd ffdec && ./ff && cd ../../ && \
+cd tests && ./a && ./c 1 && cd ffdec && ./ff && cd ../data && ./a && cd ../../ && \
 cd example && make test
--- actionswf-1.orig/oad/main.oc
+++ actionswf-1/oad/main.oc
@@ -11,21 +11,40 @@ include "common.h" "common.h.oh"
 
 import "fcontent" fcontent
 functionx oadata_make(ss swf_fullpath)
-	#.x
 	sd size;add size strlen(swf_fullpath)
-	sd allsize=2+1;add allsize size
+	char logext=".log";vstr slogext^logext
+	char xext=".x";vstr sxext^xext
+	char xxext=".xx";vstr sxxext^xxext
+	sd allsize=\.logext;add allsize size
 	sd mem;set mem malloc(allsize)
 	if mem!=(NULL)
+		sd exit
 		call memcpy(mem,swf_fullpath,size)
 		add size mem
-		char xext=".x";vstr sxext^xext
-		call memcpy(size,sxext,\.xext)
-		sd xcontent;set xcontent fcontent(mem)
-		if xcontent!=(NULL)
-			call free(xcontent)
+		call memcpy(size,slogext,\.logext)
+		sd logcontent;set logcontent fcontent(mem)
+		if logcontent!=(NULL)
+			call memcpy(size,sxext,\.xext)
+			sd xcontent;set xcontent fcontent(mem)
+			if xcontent!=(NULL)
+				call memcpy(size,sxxext,\.xxext)
+				sd xxcontent;set xxcontent fcontent(mem)
+				if xxcontent!=(NULL)
+					set exit (EXIT_SUCCESS)
+					call free(xxcontent)
+				else
+					set exit (EXIT_FAILURE)
+				end
+				call free(xcontent)
+			else
+				set exit (EXIT_FAILURE)
+			end
+			call free(logcontent)
+		else
+			set exit (EXIT_FAILURE)
 		end
 		call free(mem)
-		return (EXIT_SUCCESS)
+		return exit
 	end
 	return (EXIT_FAILURE)
 end
--- actionswf-1.orig/src/actionutil.oc
+++ actionswf-1/src/actionutil.oc
@@ -18,7 +18,7 @@ function row_termination(sv p_ac,sv p_ro
 		return 0
 	endif
 	if ac#=(CarriageReturn)
-		inc ac;if ac#=0xa;set p_ac# ac;endif
+		inc ac;if ac#=(LineFeed);set p_ac# ac;endif
 		inc p_row#
 		return 0
 	endif
--- actionswf-1.orig/src/x.oc
+++ actionswf-1/src/x.oc
@@ -8,7 +8,9 @@ import "mem_free" mem_free
 
 function x_data()
 	value f=NULL
-	datax size#1
+	datax size#1   #there was a debate on this
+	#size here is on small mem xcode. but there are 2 sizes(at xx and at xstart) that are on large xcode. multiple expands on same word size?
+	#anyway, for example var a=a[0]; is 0x40 in x, 0x12 in swf, and will error there because actionrecordheader is uint16 maximum
 	value mem=NULL
 	value flog=NULL
 	if f!=(NULL)
@@ -91,7 +93,8 @@ function actionx()
 		set size pointer#v         ;#size of escaped block that was at parse + pad if debug_x_pad flag
 		incst pointer              ##here at parse was the escaped mem
 		set x_pointer.base pointer ##for x_pointer function
-		add pointer size           ##mathpointer start equivalent, but this x has expanded caller and pointers indexes
+		add pointer size           ##and add DWORD since liboadata read size without parse
+		add pointer (DWORD)        ##mathpointer start equivalent, but this x has expanded caller and pointers indexes
 		if x_data.flog!=(NULL)
 			set x_log.base pointer
 		end
@@ -187,6 +190,10 @@ function debug_x_pointers(valuex start,v
 				call f_write(x_data.f,#zero,pad)
 			end
 		end
+		#and need to write x size to fast grab at liboadata branches without x parse
+		valuex xmark#1
+		call f_write(x_data.f,#zero,(DWORD)) #write something dummy here, size is DWORD like like x_data.size
+		set xmark f_tell(x_data.f)
 	end
 end
 
@@ -203,9 +210,18 @@ function debug_x(sd mem,sd nr)
 		mult nr (action_code_values_unit)
 		call debug_x_base(mem,nr)
 
+		valuex current#1
+		set current f_tell(x_data.f)
+		sub current debug_x_pointers.xmark
+		sub debug_x_pointers.xmark (DWORD)
+
 		call fseek(x_data.f,debug_x_pointers.mark,(SEEK_SET))
 		call fwrite(debug_x_pointers.start,debug_x_pointers.size,1,x_data.f)
-		call fseek(x_data.f,0,(SEEK_END))
+
+		call fseek(x_data.f,debug_x_pointers.xmark,(SEEK_SET))
+		call fwrite(#current,(DWORD),1,x_data.f)
+
+		call fseek(x_data.f,0,(SEEK_END)) #for next writes
 	end
 end
 
