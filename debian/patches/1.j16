+	finish(samplesorig,combo)
+
+def resume(combo,pack):
+	pnts2=pack[1];samplesorig=pack[3];testspack=pack[7]
+	pack[4],pack[5],is_done=calculate_resume(pack[0],pnts2,pack[2],samplesorig,pack[4],pack[5],pack[6],testspack)
